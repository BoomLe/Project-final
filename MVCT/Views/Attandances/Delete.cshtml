@using MVCT.Models.Attandance;
@model Attandances



<form asp-action="Delete">
    <br />
    <div class="Container border p-3">
        <h1 class="text-secondary text-center">Delete</h1>
        <input asp-for="Id" hidden />
        <hr />
        <div class="row">
            <div class="col-2">
                <label class="control-label pt-2" style="font-size:20px;">DataTime</label>
            </div>
            <div class="col-10 pb-3">
                <input disabled asp-for="DateTime" class="form-control" />

            </div>
         

            <div class="col-5 offset-2">
                <input type="submit" value="Xóa" class="btn btn-danger form-control" />

            </div>
            <div class="col-5 ">

                <a asp-controller="Attandances" asp-action="Index" class="btn-primary btn form-control ">Danh sách Check-In </a>
            </div>


        </div>

        <div>
        </div>
    </div>
</form>